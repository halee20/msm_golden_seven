<h1>Actor #<%= @actor.id %></h1>

<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= @actor.name %></dd>

  <dt>Date of Birth</dt>
  <dd><%= @actor.dob %></dd>

  <dt>Bio</dt>
  <dd><%= @actor.bio %></dd>

  <dt>Image</dt>
  <dd><img src="<%= @actor.image_url %>" width="320"></dd>

</dl>

<p><a href = "/actors">Back to All Actors</a></p>
